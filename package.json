{
  "name": "post-it",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch": "npm run watch-code & npm run watch-css",
    "watch-code": "nodemon ./bin/www",
    "build-css": "node-sass -o ./public/stylesheets -w sass",
    "watch-css": "nodemon -e sass -x \"npm run build-css\"",
    "start-parcel": "parcel ./src/client/views/index.pug",
    "dev": "concurrently --kill-others \"npm run start-parcel\" \"npm run start\""
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "bad-words": "^3.0.2",
    "core-js": "^3.1.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "interactjs": "^1.4.10",
    "regenerator": "^0.14.1",
    "mustache": ">=2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "concurrently": "^4.1.1",
    "node-sass": "^4.13.1",
    "nodemon": "^1.19.1",
    "parcel-bundler": "^1.12.3",
    "pug": "^2.0.4"
  }
}
