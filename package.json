{
  "name": "post-it",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch": "npm run watch-code & npm run watch-css",
    "watch-code": "nodemon ./bin/www",
    "build-css": "node-sass -o ./public/stylesheets -w sass",
    "watch-css": "nodemon -e sass -x \"npm run build-css\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "interactjs": "^1.4.10",
    "morgan": "~1.9.1",
    "pug": "~2.0.3"
  },
  "devDependencies": {
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1"
  }
}
